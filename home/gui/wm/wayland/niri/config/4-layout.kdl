layout {
  gaps 0
  center-focused-column "never"
  preset-column-widths {
    proportion 0.33333
    proportion 0.5
    proportion 1.0
  }
  default-column-width { }
  border {
    off
    width 4
  }
  focus-ring {
    off
    width 4
    active-gradient from="#d20f39" to="#fab387" angle=90
  }
  struts {
    left 0
    right 0
    top 0
    bottom 0
  }
}
